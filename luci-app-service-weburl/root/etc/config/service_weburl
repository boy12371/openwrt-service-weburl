# Service WebURL 配置文件

# 主配置段
config main
    # 是否启用插件 (1:启用, 0:禁用)
    option enabled '1'
    
    # 数据库文件路径
    option db_path '/var/lib/service_weburl.db'
    
    # 日志保留天数 (默认30天)
    option log_retention_days '30'
    
    # 服务缓存时间 (秒)
    option cache_time '300'
    
    # 每页显示的服务数量
    option items_per_page '20'

# 高级配置
config advanced
    # 自动清理日志的间隔 (小时)
    option cleanup_interval '24'
    
    # 是否启用调试日志 (1:启用, 0:禁用)
    option debug '0'