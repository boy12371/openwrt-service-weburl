{
    "luci-app-service-weburl": {
        "description": "Service WebURL Management Access Control",
        "read": {
            "ubus": {
                "luci": ["getConntrackList", "getInitList", "getLEDs", "getLocales", "getTime"]
            },
            "uci": ["service_weburl"]
        },
        "write": {
            "ubus": {
                "luci": ["applyConntrack", "applyLEDs", "applyLocales", "applyTime"]
            },
            "uci": ["service_weburl"],
            "file": {
                "/var/lib/service_weburl.db": ["write"],
                "/etc/config/service_weburl": ["write"]
            }
        }
    },
    "service-weburl-read": {
        "description": "Read-only access to Service WebURL",
        "read": {
            "ubus": {
                "luci.service_weburl": ["get_services", "get_logs"]
            }
        }
    },
    "service-weburl-write": {
        "description": "Full access to Service WebURL",
        "read": {
            "ubus": {
                "luci.service_weburl": ["*"]
            }
        },
        "write": {
            "ubus": {
                "luci.service_weburl": ["*"]
            },
            "file": {
                "/var/lib/service_weburl.db": ["write"],
                "/etc/config/service_weburl": ["write"]
            }
        }
    }
}